gcloud functions deploy ingest_csv \
  --gen2 \
  --runtime=python311 \
  --region=us-east4 \
  --entry-point=ingest_csv \
  --trigger-http \
  --allow-unauthenticated \
  --timeout=300s \
  --set-env-vars=INSTANCE_CONNECTION_NAME="INSERT-CONNECTION-NAME",MYSQL_USER="INSERT-USERNAME",MYSQL_PASSWORD="INSERT-PASSWORD",MYSQL_DATABASE="INSERT-DB-NAME",MYSQL_TABLE="INSERT-TABLE-NAME"
